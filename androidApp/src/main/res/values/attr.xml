<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PileCardView">
        <attr name="pile_card_setIcon" format="reference" />
        <attr name="pile_card_setLabel" format="string" />
        <attr name="pile_card_setPercent" format="integer" />
        <attr name="pile_card_setMoney" format="string" />
    </declare-styleable>

    <declare-styleable name="SalaryCardView">
        <attr name="salary_card_setIcon" format="reference" />
        <attr name="salary_card_setTitle" format="string" />
        <attr name="salary_card_setName" format="string" />
        <attr name="salary_card_setSalary" format="float" />
    </declare-styleable>

    <declare-styleable name="TextFieldWithPercentage">
        <attr name="text_field_with_percentage_setHintPrice" format="string" />
        <attr name="text_field_with_percentage_setHintPercentage" format="string" />
        <attr name="text_field_with_percentage_setDefaultPrice" format="float" />
        <attr name="text_field_with_percentage_setDefaultPercentage" format="integer" />
    </declare-styleable>


</resources>